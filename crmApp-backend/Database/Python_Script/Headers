//Ass vie
Creer Module 
	iddomaineass = 1 // Assurance Vie
	SELECT last idcontrat
	
Creer Souscription
	SELECT last_insert_id MODULE
	select idmodalite FROM MODALITE WHERE libelleavantage = 'Classe' AND iddomaineass = 1
	SELECT valeur FROM MODALITE_VALEUR WHERE (valeur = #Classe/Vie) XOR (valeur = 'Autres') // Check XOR

Creer Souscription
